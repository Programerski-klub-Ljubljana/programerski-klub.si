{
  "name": "programerski-klub.si",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate-venv": "python3 -m venv venv && ./venv/bin/pip install -r requirements.txt",
    "generate-api": "rm -fr src/api && openapi --postfix Api --exportSchemas true --input http://localhost:8000/schema --output ./src/api --client axios",
    "scripts-api": "./venv/bin/python scripts/api.py",
    "scripts-sitemap": "./venv/bin/python scripts/sitemap.py",
    "scripts-upload": "./venv/bin/python scripts/sitemap.py",
    "lint": "eslint tests --format visualstudio",
    "test": "mocha tests/**/*.js --timeout 20000",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@dimforge/rapier3d-compat": "^0.9.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.47",
    "@sveltejs/kit": "next",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.7",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "mocha": "^10.1.0",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "@threlte/core": "^4.3.2",
    "@threlte/extras": "^4.4.1",
    "@threlte/rapier": "^0.5.0",
    "axios": "^1.1.3",
    "bootstrap-icons": "^1.9.1",
    "moment": "^2.29.4",
    "papercss": "^1.8.3",
    "request-image-size": "^2.2.0",
    "three": "^0.145.0"
  }
}
