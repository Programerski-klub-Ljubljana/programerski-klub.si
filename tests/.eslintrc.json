{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-param-reassign": "off",
    "object-property-newline": "off",
    "object-curly-spacing": "off",
    "linebreak-style": "off",
    "consistent-return": "off"
  }
}
